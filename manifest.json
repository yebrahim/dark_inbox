{
   "content_scripts": [ {
      "include_globs": [ "*" ],
      "js": [ "dark_inbox.js" ],
      "matches": [ "http://inbox.google.com/u/*", "https://inbox.google.com/u/*" ],
      "run_at": "document_end"
   } ],
   "description": "Darkens the Google Inbox page to an easy on the eyes skin.",
   "manifest_version": 2,
   "name": "Google Inbox Dark Skin",
   "short_name": "Dark Inbox",
   "version": "0.1"
}
